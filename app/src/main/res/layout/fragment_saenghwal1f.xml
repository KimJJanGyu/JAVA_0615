<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.saenghwal.Saenghwal1fFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_saenghwal_1f_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="40dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_saenghwal_1f_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="290dp" />

    <ImageView
        android:id="@+id/saenghwalimage_1f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/saenghwal1f_image"
        android:contentDescription="@string/todo"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="100dp" />

    <ImageView
        android:id="@+id/overlay_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_maker"
        android:visibility="gone"
        android:contentDescription="@string/todo"
        app:layout_constraintTop_toTopOf="@id/saenghwalimage_1f"
        app:layout_constraintStart_toStartOf="@id/saenghwalimage_1f" />

    <TextView
        android:id="@+id/saenghwal1f_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/string_saenghwal1f_1"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/saenghwalimage_1f"
        app:layout_constraintStart_toStartOf="@id/guideline_start_saenghwal_1f_1"
        android:clickable="true"
        android:focusable="true" />

    <TextView
        android:id="@+id/saenghwal1f_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/string_saenghwal1f_2"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/saenghwalimage_1f"
        app:layout_constraintStart_toStartOf="@id/guideline_start_saenghwal_1f_2"
        android:clickable="true"
        android:focusable="true" />

</androidx.constraintlayout.widget.ConstraintLayout>